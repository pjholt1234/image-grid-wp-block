{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "create-block/logo-grid",
  "title": "Logo Grid",
  "category": "widgets",
  "attributes": {
    "logos": {
      "type": "array",
      "default": []
    },
    "columns": {
      "type": "number",
      "default": 3
    }
  },
  "supports": {
    "align": [
      "wide",
      "full"
    ]
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}